    "   \"source\": [\n",
    "    \"import numpy as np\\n\",\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Assuming 'supermarket' is the column indicating supermarket name\\n\",\n",
    "    \"\\n\",\n",
    "    \"def assign_promos_by_super(sp_df, promo_rate=0.2, promo_options=None):\\n\",\n",
    "    \"    if promo_options is None:\\n\",\n",
    "    \"        promo_options = [None, \\\"10% off\\\", \\\"Buy 1 Get 1\\\", \\\"5 NIS discount\\\", \\\"5% off\\\"]\\n\",\n",
    "    \"\\n\",\n",
    "    \"    promo_flags = []\\n\",\n",
    "    \"    promo_descs = []\\n\",\n",
    "    \"\\n\",\n",
    "    \"    # Apply per supermarket\\n\",\n",
    "    \"    for market, group in sp_df.groupby('supermarket'):\\n\",\n",
    "    \"        n = len(group)\\n\",\n",
    "    \"        # Number of promos to assign\\n\",\n",
    "    \"        promo_n = int(np.round(n * promo_rate))\\n\",\n",
    "    \"        promo_idx = np.random.choice(n, promo_n, replace=False)\\n\",\n",
    "    \"\\n\",\n",
    "    \"        flag = np.zeros(n, dtype=int)\\n\",\n",
    "    \"        flag[promo_idx] = 1\\n\",\n",
    "    \"\\n\",\n",
    "    \"        # Assign descriptions only to promos, None otherwise\\n\",\n",
    "    \"        desc = [None] * n\\n\",\n",
    "    \"        for idx in promo_idx:\\n\",\n",
    "    \"            desc[idx] = np.random.choice([p for p in promo_options if p])\\n\",\n",
    "    \"\\n\",\n",
    "    \"        promo_flags.extend(flag)\\n\",\n",
    "    \"        promo_descs.extend(desc)\\n\",\n",
    "    \"\\n\",\n",
    "    \"    sp_df = sp_df.copy()\\n\",\n",
    "    \"    sp_df['promo_flag'] = promo_flags\\n\",\n",
    "    \"    sp_df['promo_desc'] = promo_descs\\n\",\n",
    "    \"    return sp_df\\n\",\n",
    "    \"\\n\",\n",
    "    \"\\n\",\n",
    "    \"sp_df = assign_promos_by_super(sp_df, promo_rate=0.2)\\n\",\n",
    "    \"sp_df.head(10)\"\n",
